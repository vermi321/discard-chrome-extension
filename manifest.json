{
  "name": "Discard active tab or group",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Click to discard",
    "default_icon": {
      "32": "icons/32.png",
      "72": "icons/72.png",
      "128": "icons/128.png",
      "512": "icons/512.png"
    }
  },
  "permissions": ["activeTab", "tabs"],
  "background": {
    "service_worker": "background.js"
  }
}